            "oid": "upsPhaseOutputPercentPower.1.1.1",
            "type": "value",
            "units": "%Watts"
        },
        {
            "name": "Output Percent Power L2",
            "oid": "upsPhaseOutputPercentPower.1.1.2",
            "type": "value",
            "units": "%Watts"
        },
        {
            "name": "Output Percent Power L3",
            "oid": "upsPhaseOutputPercentPower.1.1.3",
            "type": "value",
            "units": "%Watts"
        },

        {
            "name": "Output Current L1",
            "oid": "upsPhaseOutputCurrent.1.1.1",
            "type": "value",
            "units": "Amps",
            "scale": 0.1
        },
        {
            "name": "Output Current L2",
            "oid": "upsPhaseOutputCurrent.1.1.2",
            "type": "value",
            "units": "Amps",
            "scale": 0.1
        },
        {
            "name": "Output Current L3",
            "oid": "upsPhaseOutputCurrent.1.1.3",
            "type": "value",
            "units": "Amps",
            "scale": 0.1
        },

        {
            "name": "Output Frequency",
            "oid": "upsPhaseOutputFrequency.1",
            "type": "value",
            "units": "Hz",
            "scale": 0.1
        },

