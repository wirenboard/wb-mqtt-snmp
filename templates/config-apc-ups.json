            "name": "Battery Capacity",
            "oid": "upsHighPrecBatteryCapacity.0",
            "type": "value",
            "units": "%",
            "scale": 0.1
        },
        {
            "name": "Nominal Battery Voltage",
            "oid": "upsHighPrecBatteryNominalVoltage.0",
            "type": "voltage",
            "scale": 0.1
        },
        {
            "name": "Actual Battery Bus Voltage",
            "oid": "upsHighPrecBatteryActualVoltage.0",
            "type": "voltage",
            "scale": 0.1
        },
        {
            "name": "Battery Current",
            "oid": "upsHighPrecBatteryCurrent.0",
            "type": "value",
            "units": "Amps",
            "scale": 0.1
        },

        {
            "name": "Number of Batteries",
            "oid": "upsAdvBatteryNumOfBattPacks.0",
            "type": "value"
        },
        {
            "name": "Number of Bad Batteries",
            "oid": "upsAdvBatteryNumOfBadBattPacks.0",
            "type": "value"
        },

        {
            "name": "Output Status",
            "oid": "upsBasicOutputStatus.0",
            "type": "text"
        },

        {
            "name": "Firmware Revision",
            "oid": "upsDiagIMFirmwareRev.1",
            "type": "text",
            "poll_interval": 10000
        },
        {
            "name": "Serial Number",
            "oid": "upsDiagIMSerialNum.1",
            "type": "text",
            "poll_interval": 10000
        },
        {
            "name": "Manufacture Date",
            "oid": "upsDiagIMManufactureDate.1",
            "type": "text",
            "poll_interval": 10000
        },
        {
            "name": "Hardware Revision",
            "oid": "upsDiagIMHardwareRev.1",
            "type": "text",
            "poll_interval": 10000
        }
