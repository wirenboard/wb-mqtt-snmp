{
    "device_type": "apc-ups",
    "snmp_version": "2c",
    "snmp_timeout": 5,
    "oid_prefix": "PowerNet-MIB",
    "channels": [
        {
            "name": "Internal temperature",
            "oid": "upsHighPrecBatteryTemperature.0",
            "type": "temperature",
            "scale": 0.1
        },
        {
            "name": "Runtime Remaining",
            "oid": "upsAdvBatteryRunTimeRemaining.0",
            "type": "value" // DURATION
        },
        
        {
            "name": "Input Voltage L1",
            "oid": "upsPhaseInputVoltage.1.1.1",
            "type": "voltage" // ???
        },
        {
            "name": "Input Voltage L2",
            "oid": "upsPhaseInputVoltage.1.1.2",
            "type": "voltage" // ???
        },
        {
            "name": "Input Voltage L3",
            "oid": "upsPhaseInputVoltage.1.1.3",
            "type": "voltage" // ???
        },

        {
            "name": "Bypass Input Voltage L1",
            "oid": "upsPhaseInputVoltage.2.1.1",
            "type": "voltage"
        },
        {
            "name": "Bypass Input Voltage L2",
            "oid": "upsPhaseInputVoltage.2.1.2",
            "type": "voltage"
        },
        {
            "name": "Bypass Input Voltage L3",
            "oid": "upsPhaseInputVoltage.2.1.3",
            "type": "voltage"
        },

        {
            "name": "Output Voltage L1",
            "oid": "upsPhaseOutputVoltage.1.1.1",
            "type": "voltage"
        },
        {
            "name": "Output Voltage L2",
            "oid": "upsPhaseOutputVoltage.1.1.2",
            "type": "voltage"
        },
        {
            "name": "Output Voltage L3",
            "oid": "upsPhaseOutputVoltage.1.1.3",
            "type": "voltage"
        },

        {
            "name": "Current L1",
            "oid": "upsPhaseInputCurrent.1.1.1",
            "type": "value",
            "units": "Amps",
            "scale": 0.1
        },
        {
            "name": "Current L2",
            "oid": "upsPhaseInputCurrent.1.1.2",
            "type": "value",
            "units": "Amps",
            "scale": 0.1
        },
        {
            "name": "Current L3",
            "oid": "upsPhaseInputCurrent.1.1.3",
            "type": "value",
            "units": "Amps",
            "scale": 0.1
        },

        {
            "name": "Input Frequency",
            "oid": "upsHighPrecInputFrequency.0",
            "type": "value",
            "units": "Hz",
            "scale": 0.1
        },

        {
            "name": "Output Load L1",
            "oid": "upsPhaseOutputLoad.1.1.1",
            "type": "value",
            "units": "kVA",
            "scale": 0.001
        },
        {
            "name": "Output Load L2",
            "oid": "upsPhaseOutputLoad.1.1.2",
            "type": "value",
            "units": "kVA",
            "scale": 0.001
        },
        {
            "name": "Output Load L3",
            "oid": "upsPhaseOutputLoad.1.1.3",
            "type": "value",
            "units": "kVA",
            "scale": 0.001
        },

        {
            "name": "Output Percent Load L1",
            "oid": "upsPhaseOutputPercentLoad.1.1.1",
            "type": "value",
            "units": "%kVA"
        },
        {
            "name": "Output Percent Load L2",
            "oid": "upsPhaseOutputPercentLoad.1.1.2",
            "type": "value",
            "units": "%kVA"
        },
        {
            "name": "Output Percent Load L3",
            "oid": "upsPhaseOutputPercentLoad.1.1.3",
